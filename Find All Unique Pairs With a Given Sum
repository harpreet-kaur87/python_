   "source": [
    "Problem Statement\n",
    "\n",
    "Given a list of integers and a target sum, write a Python program to find all unique pairs of elements in the list whose sum is equal to the given target."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7e251553-29b1-42af-b228-88f04f9ddb1b",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "[[2, 5], [3, 4]]"
      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "nums = [2, 4, 3, 5, 7, 8, 9]\n",
    "target = 7\n",
    "\n",
    "def find_pairs(nums,target):\n",
    "    \n",
    "    pairs = []\n",
    "    \n",
    "    for i in range(len(nums)):\n",
    "        for j in range(i+1, len(nums)):\n",
    "            if nums[i] + nums[j] == target:\n",
    "                pair = sorted((nums[i],nums[j]))\n",
    "                if pair not in pairs:\n",
    "                    pairs.append(pair)\n",
    "    return pairs\n",
    "        \n",
    "result = find_pairs(nums,target)\n",
    "result"
   ]
  }
